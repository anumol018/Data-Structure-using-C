#include<stdio.h>
void main()
{
 int n,flag=0,i,x,count=0;
 count+=2;
 printf("enter the limit:");
 scanf("%d",&n);
 count++;
 int A[n];
 printf("enter the values:");
 for(i=0;i<n;i++)
 {
  count++;
  scanf("%d",&A[i]);
  count++;
 }
 printf("enter the number to be checked:");
 scanf("%d",&x);
 count++;
 for(i=0;i<n;i++)
 {
  count++;
  if(A[i]==x)
  {
  count++;
   flag++;
   count++;
  }
 }
 if(flag>0)
 {
 count++;
  printf("%d is present in the array\n",x);
  count++;
 }
 else
 {
  printf("%d is not present in the array\n",x);
  count++;
 }
 printf("space complexity=%d\n",(20+(4*n)));
 count+=2;
 printf("time complexity=%d\n",count);
}
